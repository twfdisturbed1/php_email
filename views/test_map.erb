<html>
<head>
    <style>
      #map {
          width: 100%;
          height: 400px;
          background-color: grey;
        }
    </style>
</head>

<body>
    <h3>Google Maps Test Page</h3>
    <div id="map"></div>

    <script>
      // Initialize and add the map
      function initMap() {
        // The location of Uluru (Serenity Pointe)
        var uluru = {lat: 37.675758, lng: -82.280603};
        // The map, centered at Uluru (Serenity Pointe)
        var map = new google.maps.Map(
            document.getElementById('map'), {zoom: 15, center: uluru});
        // The marker, positioned at Uluru (Serenity Pointe)
        var marker = new google.maps.Marker({position: uluru, map: map});
      }
    </script>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDSv_hcW7NS7AhdRX_FtIwwnyQvMVDx5TY&callback=initMap"
    async defer></script>
</body>
</html>